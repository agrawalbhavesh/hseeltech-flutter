  "legalRiskS1Title": "Market Risk",
  "@legalRiskS1Title": {
    "description": "legal.risk.s1.title"
  },
  "legalRiskS1Icon": "ðŸ“‰",
  "@legalRiskS1Icon": {
    "description": "legal.risk.s1.icon"
  },
  "legalRiskS1Body": "Real estate values are subject to fluctuations due to economic conditions, market supply and demand, interest rates, and other factors. The value of your investment may decrease, and you may receive less than your original investment amount upon exit.",
  "@legalRiskS1Body": {
    "description": "legal.risk.s1.body"
  },
  "legalRiskS2Title": "Return Risk",
  "@legalRiskS2Title": {
    "description": "legal.risk.s2.title"
  },
  "legalRiskS2Icon": "ðŸ“Š",
  "@legalRiskS2Icon": {
    "description": "legal.risk.s2.icon"
  },
  "legalRiskS2Body": "All returns displayed on the Platform are estimated/expected returns based on projections and historical data. These are NOT guaranteed. Actual returns may be significantly lower than projected, or you may receive no returns at all. Factors affecting returns include:\n\nâ€¢ Vacancy rates and tenant defaults.\nâ€¢ Property maintenance and unexpected repair costs.\nâ€¢ Changes in rental market conditions.\nâ€¢ Property management performance.\nâ€¢ Economic downturns affecting property values.",
  "@legalRiskS2Body": {
    "description": "legal.risk.s2.body"
  },
  "legalRiskS3Title": "Liquidity Risk",
  "@legalRiskS3Title": {
    "description": "legal.risk.s3.title"
  },
  "legalRiskS3Icon": "ðŸ”’",
  "@legalRiskS3Icon": {
    "description": "legal.risk.s3.icon"
